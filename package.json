{
  "name": "feathers-graph-populate",
  "description": "Add lightning fast, GraphQL-like populates to your FeathersJS API.",
  "version": "4.0.0",
  "homepage": "https://feathers-graph-populate.netlify.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/marshallswain/feathers-graph-populate"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "feathers",
    "feathers.js",
    "feathers-plugin",
    "hooks",
    "populate"
  ],
  "author": {
    "name": "Marshall Thompson",
    "email": "marshall@creativeideal.net"
  },
  "contributors": [],
  "bugs": {},
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/**",
    "dist/**"
  ],
  "scripts": {
    "compile": "shx rm -rf dist/ && tsup src/index.ts --format cjs,esm --dts",
    "version": "npm run compile",
    "release": "np",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "vitest run",
    "vitest": "vitest",
    "coverage": "vitest run --coverage",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "peerDependencies": {
    "@feathersjs/feathers": "^5.0.0"
  },
  "dependencies": {
    "@feathersjs/commons": "^5.0.8",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@feathersjs/errors": "^5.0.8",
    "@feathersjs/feathers": "^5.0.8",
    "@feathersjs/memory": "^5.0.8",
    "@types/lodash": "^4.14.198",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "np": "^8.0.4",
    "shx": "^0.3.4",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.5",
    "vuepress": "^1.9.10",
    "vuepress-theme-default-prefers-color-scheme": "^2.0.0"
  }
}
