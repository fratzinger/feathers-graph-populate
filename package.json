{
  "name": "feathers-graph-populate",
  "description": "Add lightning fast, GraphQL-like populates to your FeathersJS API.",
  "version": "3.0.1",
  "homepage": "https://feathers-graph-populate.netlify.app/",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "feathers",
    "feathers.js",
    "feathers-plugin",
    "hooks",
    "populate"
  ],
  "author": {
    "name": "Marshall Thompson",
    "email": "marshall@creativeideal.net"
  },
  "contributors": [],
  "bugs": {},
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/**",
    "lib/**",
    "dist/**"
  ],
  "scripts": {
    "compile": "shx rm -rf dist/ && tsup lib/index.ts --format cjs,esm --dts",
    "version": "npm run compile",
    "release": "np",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "vitest run",
    "vitest": "vitest",
    "coverage": "vitest run --coverage",
    "docs": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "@feathersjs/commons": "^5.0.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@feathersjs/errors": "^5.0.1",
    "@feathersjs/feathers": "^5.0.1",
    "@feathersjs/memory": "^5.0.1",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "mocha": "^10.2.0",
    "np": "^7.6.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.2.0",
    "vitest": "^0.29.3",
    "vuepress": "^1.9.9",
    "vuepress-theme-default-prefers-color-scheme": "^2.0.0"
  }
}
